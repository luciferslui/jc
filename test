    <script type="text/javascript">
        loadLangPreference();

        function reInit() {
            $(window).scrollTop(0);
            supportAutoScrolling = false;
            $('#printDiv').html('');

            switchProduct(curProduct, curLang);

            clearAllInterval();

            curPageId = window.location.pathname.toLowerCase().replace('/' + curProduct + '/', '').replace('.aspx', '')
                .replace('/index', 'index');
            callWAReq();
        }

        $(document).ready(function () {
            if (isQRPortal) {
                startQRPortal();
            } else {

                loadEwinPara(ewinParaPath);

                $('#infoDiv').click(function () {
                    if (isSlipOpen() && betslipState != STATE_IDLEALERT) {
                        slipClose(true);
                    }
                });
            }
            genMenuObject(curLang);
            logonDetectForOddsPush();
            mobileBrowserChecking();
            pushFirstHistory();
            pageScroll();
            showBetPreviewOnEnter();
        });

        var checkChangesEnabled = true;
        if (!isQRPortal) {
            window.onbeforeunload = function () {
                if (!checkChangesEnabled)
                    return;

                var returnValue = null;
                if (isLogon() || betslipState == STATE_EKBA) {
                    returnValue = GetError("1303");
                } else if (isBetBasketNotEmpty())
                    returnValue = GetError("refresh_warning");

                if (returnValue != null) {
                    return returnValue;
                }
            };

            window.onunload = function () {
                if (isLogon() || betslipState == STATE_EKBA) {
                    if (typeof bypassForceLogout == 'undefined' || bypassForceLogout != true) {
                        forceLogout();
                    }
                }
                CloseAllPopup(true);
            }
        }
    </script>
