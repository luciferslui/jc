                                <script type="text/javascript">
                                    var showInvComingleMsg = true;

                                    function poolInvPoolTotLoadDoc() {
                                        poolInvPoolTotRefreshDoc(jsonURL + '?type=pooltot&date=' + mtgDate + '&venue=' + mtgVenue + '&raceno=' + curRaceNo);

                                    }


                                    function poolInvPoolTotRefreshDoc(url, retryOnce) {
                                        $.get(url, function (data) {
                                            if (data.indexOf && data.indexOf(wafKeyword) >= 0 && !retryOnce) {
                                                $('#tFrame').attr('src', '/reload.aspx?a=' + (new Date().getTime()));
                                                setTimeout(function () {
                                                    poolInvPoolTotRefreshDoc(url, true);
                                                }, 1000);
                                                return;
                                            }

                                            poolInvPoolTotRefreshOddsJson(data)
                                        }
                                        ).fail(function () { window.setTimeout('poolInvPoolTotRefreshDoc()', 400); });
                                    }

                                    function poolInvPoolTotRefreshOddsJson(json) {
                                        var nodes = json.inv;
                                        var singleTotal = 0;
                                        var allTotal = 0;
                                        var oddsTime = json.updateTime;
                                        updateRefreshTime(oddsTime);

                                        var singlePoolList = 'WIN;PLA;QIN;QPL;IWN;CWA;CWB;CWC;FCT;TCE;TRI;F-F;QTT';
                                        var pid = [];
                                        for (var i = 0; i < nodes.length; i++) {
                                            var node = nodes[i];
                                            var pool = node.pool;
                                            var investment = node.value;
                                            try {
                                                $('#poolInvPoolTot' + pool).html('$ ' + addComma(investment));
                                                if (!isNaN(investment)) {
                                                    if (typeof (node.pid) == 'undefined') {
                                                        if (singlePoolList.indexOf(pool) >= 0) {
                                                            singleTotal += parseInt(investment, 10);
                                                        }
                                                        allTotal += parseInt(investment, 10);
                                                    } else if (pid.indexOf(node.pid) < 0) {
                                                        if (singlePoolList.indexOf(pool) >= 0) {
                                                            singleTotal += parseInt(investment, 10);
                                                        }
                                                        allTotal += parseInt(investment, 10);
                                                        pid.push(node.pid);
                                                    }
                                                }
                                            } catch (e) {

                                            }
                                        }
                                        $('#poolInvPoolTotSUM').html('<strong class="racingBold">$ ' + addComma(singleTotal.toString()) + '</strong>');
                                        $('#poolInvPoolTotTOTALSUM').html('<strong class="racingBold">$ ' + addComma(allTotal.toString()) + '</strong>');
                                        $('#poolInvPoolTotGRANDSUM').html('<strong class="racingBold">$ ' + addComma(json.totalInv) + '</strong>');
                                    }



                                    /**************************************************/
                                    /*            for odds push                       */
                                    /**************************************************/
                                    var startTotalInvRefresh = false;

                                    function poolInvPoolTotRefreshOddsOddsPush() {
                                        //var stringTest = "153029@@@;WIN=1804284.0;PLA=471018.0;QIN=697767.0;QPL=218408.0;TRI=332132.0;DBL=129320.0;TCE=83780.0@@@;WIN=1117447.0;PLA=857831.0;QIN=266870.0;QPL=190801.0;TRI=229233.0;DBL=373550.0;TCE=16690.0@@@;WIN=287131.0;PLA=325558.0;QIN=598205.0;QPL=739841.0;TRI=59480.0;DBL=77940.0;TCE=232503.0@@@;WIN=114419.0;PLA=99981.0;QIN=73893.0;QPL=357626.0;TRI=36844.0;DBL=52560.0;TCE=334944.0@@@;WIN=181070.0;PLA=163687.0;QIN=267015.0;QPL=45877.0;TRI=18774.0;DBL=4140.0;TCE=18785.0@@@;WIN=38400.0;PLA=20680.0;QIN=151460.0;QPL=145686.0;TRI=10888.0;DBL=116940.0;TCE=172090.0@@@;WIN=17510.0;PLA=18030.0;QIN=48330.0;QPL=37350.0;TRI=4982.0;DBL=53640.0;TCE=121228.0@@@;WIN=6902423.0;PLA=28713.0;QIN=236191.0;QPL=97913.0;TRI=42610.0;TCE=31120.0@@@";
                                        var tmpArray = meetingPushData['POOL_TOTAL'].split('@@@');
                                        if (tmpArray.length <= curRaceNo) {
                                            return;
                                        }

                                        var singleTotal = 0;
                                        var allTotal = 0;
                                        var grandTotal = 0;
                                        var invPoolList = [].concat(singleRacePool).concat(multiRacePool);
                                        var winMergeID = mergedPoolName['WIN_' + curRaceNo];
                                        var winMerge = winMergeID != null;
                                        var fctTceTriMergeID = mergedPoolName['FCT_' + curRaceNo] != null ? mergedPoolName['FCT_' + curRaceNo] : mergedPoolName['TCE_' + curRaceNo];
                                        var fctTceTriMerge = fctTceTriMergeID != null;
                                        var qttMergeID = mergedPoolName['QTT_' + curRaceNo];
                                        var qttMerge = qttMergeID != null;

                                        var poolInv = {};
                                        var totalArray = tmpArray[curRaceNo].split(';');
                                        for (var i = 0; i < totalArray.length; i++) {
                                            var node = totalArray[i];
                                            if (node == '')
                                                continue;

                                            var pool = node.split('=')[0];
                                            var inv = node.split('=')[1];
                                            poolInv[pool] = inv;
                                        }

                                        for (var idx in invPoolList) {
                                            var pool = invPoolList[idx];
                                            inv = poolInv[pool];
                                            if (winMerge && (pool == 'WIN' || pool == 'CWA' || pool == 'CWB' || pool == 'CWC' || pool == 'IWN')) {
                                                if (winMergeID == mergedPoolName[pool + '_' + curRaceNo])
                                                    inv = poolInv[winMergeID];
                                            }

                                            if (fctTceTriMerge && (pool == 'FCT' || pool == 'TCE' || pool == 'TRI')) {
                                                if (fctTceTriMergeID == mergedPoolName[pool + '_' + curRaceNo])
                                                    inv = poolInv[fctTceTriMergeID];
                                            }

                                            if (qttMerge && (pool == 'F-F' || pool == 'QTT')) {
                                                if (qttMergeID == mergedPoolName[pool + '_' + curRaceNo])
                                                    inv = poolInv[qttMergeID];
                                            }
                                            if ($('#poolInvPoolTot' + pool).length > 0) {
                                                $('#poolInvPoolTot' + pool).html('$ ' + addComma(inv));
                                                if (singleRacePool.indexOf(pool) >= 0)
                                                    singleTotal += parseInt(inv, 10);
                                                allTotal += parseInt(inv, 10);
                                            }
                                        }

                                        $('#poolInvPoolTotSUM').html('<strong class="racingBold">$ ' + addComma(singleTotal.toString()) + '</strong>');
                                        $('#poolInvPoolTotTOTALSUM').html('<strong class="racingBold">$ ' + addComma(allTotal.toString()) + '</strong>');


                                        if (!startTotalInvRefresh) {
                                            poolInvAutoRefreshTotalInv();
                                            startTotalInvRefresh = true;
                                        }

                                    }

                                    function handleOddPushForPoolTotal() {
                                        poolInvPoolTotRefreshOddsOddsPush();
                                    }



                                    function poolInvAutoRefreshTotalInv() {
                                        poolInvTotalInvLoadDoc();
                                        setTimeout("poolInvAutoRefreshTotalInv()", 30000);
                                    }

                                    function poolInvTotalInvLoadDoc(retryOnce) {

                                        var url = jsonURL + '?type=pooltot&date=' + mtgDate + '&venue=' + mtgVenue + '&raceno=' + curRaceNo;
                                        $.get(url, function (data) {
                                            if (data.indexOf && data.indexOf(wafKeyword) >= 0 && !retryOnce) {
                                                $('#tFrame').attr('src', '/reload.aspx?a=' + (new Date().getTime()));
                                                setTimeout(function () {
                                                    poolInvTotalInvLoadDoc(true);
                                                }, 1000);
                                                return;
                                            }
                                            poolInvTotalInvRefreshOdds(data)
                                        }).fail(function () { window.setTimeout('poolInvTotalInvLoadDoc()', 400); });

                                    }

                                    function poolInvTotalInvRefreshOdds(json) {
                                        $('#poolInvPoolTotGRANDSUM').html('<strong class="racingBold">$ ' + addComma(json.totalInv) + '</strong>');
                                    }


                                </script>

