        function browserCheck() {
            "use strict";
            try { eval("var foo = (x)=>x+1"); }
            catch (e) { return false; }
            return true;
        }

        var UNSUPPORTED_BROWSER = 'IE|IEMobile';
        var UNSUPPORTED_BROWSER_REDIRECT = "https://is.hkjc.com/jcbw/SplashScn/legacy/mass/{0}.asp".replace("{0}", window.location.href.indexOf("lang=en") >= 0 ? "en" : "ch");
        var uaParser = new UAParser();
        if (window.location.href.indexOf("b=1") < 0) {
            if (!browserCheck() || UNSUPPORTED_BROWSER.indexOf(uaParser.getBrowser().name) >= 0) {
                window.location.href = UNSUPPORTED_BROWSER_REDIRECT;
            }
        }
