                        <script type="text/javascript">
                            ///////////////////////////////////////////////
                            // UI Related
                            ///////////////////////////////////////////////
                            function switchAllUpPage() {
                                var allupObj = {};
                                var selectPoolCnt = 0;

                                if (wpTableRS.getSelCount('WIN') > 0) {
                                    allupObj = {
                                        bType: 'WIN',
                                        legSels: wpTableRS.state.legSels['WIN'],
                                        fieldSels: wpTableRS.state.fieldSels['WIN']
                                    };
                                    selectPoolCnt++;
                                }
                                if (wpTableRS.getSelCount('PLA') > 0) {
                                    allupObj = {
                                        bType: 'PLA',
                                        legSels: wpTableRS.state.legSels['PLA'],
                                        fieldSels: wpTableRS.state.fieldSels['PLA']
                                    };
                                    selectPoolCnt++;
                                }
                                if (wpTableRS.getSelCount('W-P') > 0) {
                                    allupObj = {
                                        bType: 'W-P',
                                        legSels: wpTableRS.state.legSels['W-P'],
                                        fieldSels: wpTableRS.state.fieldSels['W-P']
                                    };
                                    selectPoolCnt++;
                                }

                                if (selectPoolCnt > 1) {
                                    alert(onePoolOnlyLbl);
                                    $('#allupCheckbox').prop('checked', false);
                                    return;
                                }
                                else if (selectPoolCnt == 1) {
                                    SetDataStore('alupSel', JSON.stringify([allupObj]));
                                }

                                var para = {};
                                para['date'] = mtgDate;
                                para['venue'] = mtgVenue;
                                para["raceno"] = curRaceNo;
                                switchTo('racing', 'pages/odds_wp_alup', curLang, para);
                            }

                            function initOddsTable() {
                                try {
                                    redrawWinPlaceTable();
                                    loadRaceColorIndex();
                                }
                                catch (e) {
                                    console.log(e);
                                }
                                displayComingleMessage();
                                redrawRSCal();
                            }

                            function displayComingleMessage() {
                                $('#cbMsg').html('');
                                $('#cbTable').html('');
                                $('#cbRemark1').html('');
                                $('#cbRemark2').html('');

                                var lg = curLang == "ch" ? "C" : "E";
                                if (obStatusByRace[curRaceNo] != '') {
                                    var msgs = obMsg[obStatusByRace[curRaceNo]];
                                    if (obCoupleByRace[curRaceNo] != null && obCoupleByRace[curRaceNo] != '') {
                                        if (msgs["NOTE1_" + lg] != '')
                                            $('#cbMsg').html(msgs["NOTE1_" + lg]);
                                        $('#cbTable').html(getCoupleTable(curRaceNo));
                                    }
                                    if (msgs["NOTE2_" + lg] != '')
                                        $('#cbRemark1').html(msgs["NOTE2_" + lg]);

                                    if (showInvComingleMsg) {
                                        var remStr = new StringBuffer();
                                        if (msgs["NOTE3_" + lg] != '')
                                            remStr.append('<div style="padding:2px 2px 2px 2px">').append(msgs["NOTE3_" + lg]).append('</div>');
                                        if (msgs["NOTE4_" + lg] != '')
                                            remStr.append('<div style="padding:2px 2px 2px 2px">').append(msgs["NOTE4_" + lg]).append('</div>');
                                        $('#cbRemark2').html(remStr.toString());
                                    }
                                }
                            }

                            ///////////////////////////////////////////////
                            // Add to betslip related
                            ///////////////////////////////////////////////
                            var ret = 1;
                            function processBet(toComponent) {
                                var noOfBetTypeSelected = 0;
                                if (isQRPortal) {
                                    if ($.inArray(true, wpTableRS.state.legSels['WIN']) > -1 || wpTableRS.state.fieldSels['WIN'])
                                        noOfBetTypeSelected++;
                                    if ($.inArray(true, wpTableRS.state.legSels['PLA']) > -1 || wpTableRS.state.fieldSels['PLA'])
                                        noOfBetTypeSelected++;
                                    if ($.inArray(true, wpTableRS.state.legSels['W-P']) > -1 || wpTableRS.state.fieldSels['W-P'])
                                        noOfBetTypeSelected++;
                                    if (noOfBetTypeSelected > 1) {
                                        alert(only1BetAllowLbl);
                                        return false;
                                    }
                                }
                                var isSuccess = false;
                                var betObj1 = processSinglePoolBet(toComponent, 'WIN');
                                var betstatus1 = ret;
                                var betObj2 = processSinglePoolBet(toComponent, 'PLA');
                                var betstatus2 = ret;
                                var betObj3 = processSinglePoolBet(toComponent, 'W-P');
                                var betstatus3 = ret;
                                var betCnt = betObj1.getSelectionSize()
                                    + betObj2.getSelectionSize()
                                    + betObj3.getSelectionSize();

                                isSuccess = true;
                                if (betCnt > 0) {
                                    if (toComponent != 'betslip') {
                                        updateBetCalDisplay(toComponent, betCnt);
                                    }
                                }
                                else {
                                    alert(insuffSLbl.replace('#', curRaceNo));
                                    isSuccess = false;
                                }

                                if (betstatus1 != 1 || betstatus2 != 1 || betstatus3 != 1) {
                                    isSuccess = false;
                                }

                                return isSuccess;
                            }

                            function processSinglePoolBet(toComponent, poolS) {
                                var betObj = wpTableRS.createBetObject(poolS);
                                ret = 1;
                                if (toComponent == 'betslip' && betObj.getSelectionSize() > 0) {
                                    ret = addSelExFO(betObj);
                                    if (ret == 1) {
                                        wpTableRS.clearCheckBox(poolS);
                                    }
                                }
                                return betObj;
                            }

                            ///////////////////////////////////////////////
                            // Odds Auto Refresh Related
                            ///////////////////////////////////////////////
                            function refreshAllDoc() {
                                scratchLoadDoc();
                                poolInvPoolTotLoadDoc();
                            }

                            function refreshOddsDoc() {
                                wpOddsLoadDoc(curRaceNo, curRaceNo, dataType);
                            }

                            function changeDataType(type) {
                                if (dataType != type) {
                                    $('#oddsRefreshTime').html('');
                                }
                                dataType = type;
                                if (oddsPushStatus != 'push' || dataType != '') {
                                    refreshOddsDoc();
                                }
                                else {
                                    updateRacingPushDisplay();
                                }
                            }

                        </script>

