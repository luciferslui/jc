<script type="text/javascript">
    var enableAccessControl = false;
    function checkAccessControl() {
        if (enableAccessControl && window.sessionStorage.getItem('is_logon') != 1) {
            window.location.replace("/general_index.aspx?lang=ch");
            return true;
        }
        return false;
    }
    if (location.href.indexOf('general_index.aspx') < 0) {
        checkAccessControl();
    }
</script>


